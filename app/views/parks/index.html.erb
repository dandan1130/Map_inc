<% provide(:title, 'Search') %>

<h1>Search</h1>


<div class-"search-box">
     <%= form_tag(parks_path, :method => 'get' ) do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :shop_name => nil %>
<% end %>
</div>
<ul><% @park_lists.each do |park| %>
  <li>
    <li>
      <%= image_tag park.park_image %>
    <%= park.park_discription %>
    <%= link_to park.park_title, park_path(park) %>
    
  </li>
 
  </li>
    <% end %>
  </ul>


