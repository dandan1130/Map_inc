<% provide(:title, 'Search') %>

<h1>Search</h1>
<%= will_paginate %>
<div class-"search-box">
     <%= form_tag(parks_path, :method => 'get' ) do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :name => nil %>
<% end %>
</div>
<ul class="users">
  <%= render @parks %>
</ul>

<%= will_paginate %>
